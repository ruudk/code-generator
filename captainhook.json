{
    "commit-msg": {
        "enabled": true,
        "actions": []
    },
    "pre-commit": {
        "enabled": true,
        "actions": [
            {
                "action": "\\CaptainHook\\App\\Hook\\PHP\\Action\\Linting"
            },
            {
                "action": "\\Ruudk\\CodeGenerator\\Hooks\\ValidateReadmeCodeSnippets"
            },
            {
                "action": "vendor/bin/php-cs-fixer fix"
            },
            {
                "action": "vendor/bin/phpstan"
            },
            {
                "action": "vendor/bin/phpunit"
            }
        ]
    }
}
